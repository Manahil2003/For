main() {
  stdout.write("Enter number:");
  var nInstr = stdin.readLineSync();
  var n = int.tryParse(nInstr!);

  var num = n;
  var high = n! % 10;
  var low = n % 10;
  var rem;

  n = n ~/ 10;

  for (int i = n; i >= 1; i = 1 ~/ 10) {
    rem = i % 10;
    if (rem > high) {
      high = rem;
    }
    if (rem < high) {
      low = rem;
    }
    print("Highest digit : $high");
    print("Lowest digit : $low");
  }
}
